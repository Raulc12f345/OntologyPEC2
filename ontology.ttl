@prefix : <http://smarttourismontology.org#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://smarttourismontology.org#> .

<http://smarttourismontology.org> rdf:type owl:Ontology ;
                                   owl:versionIRI <http://smarttourismontology.org/1.0.0> ;
                                   <http://purl.org/dc/terms/created> "2025-04-20"^^xsd:date ;
                                   <http://purl.org/dc/terms/creator> "Raúl Castillo Hernández" ;
                                   <http://purl.org/dc/terms/license> <https://creativecommons.org/licenses/by/4.0/> ;
                                   rdfs:comment "An ontology for representing smart tourism data and recommendations by place." ;
                                   rdfs:label "Smart Tourism Ontology" ;
                                   owl:versionInfo "v1.0.0" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/created
<http://purl.org/dc/terms/created> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty .


###  http://smarttourismontology.org#hasName
:hasName rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#cardinality
owl:cardinality rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://smarttourismontology.org#hasAttraction
:hasAttraction rdf:type owl:ObjectProperty ;
               owl:inverseOf :isAttractionOf ;
               rdfs:domain :City ;
               rdfs:range :Attraction ;
               rdfs:comment "Lists attractions available in a city." ;
               rdfs:label "hasAttraction" .


###  http://smarttourismontology.org#hasCity
:hasCity rdf:type owl:ObjectProperty ;
         rdfs:domain :TourismEntity ;
         rdfs:range :City ;
         rdfs:comment "Links an entity to the city where it is located." ;
         rdfs:label "hasCity" .


###  http://smarttourismontology.org#hasEvent
:hasEvent rdf:type owl:ObjectProperty ;
          owl:inverseOf :isEventOf ;
          rdfs:domain :City ;
          rdfs:range :Event ;
          rdfs:comment "States which events take place in a city." ;
          rdfs:label "hasEvent" .


###  http://smarttourismontology.org#hasHotel
:hasHotel rdf:type owl:ObjectProperty ;
          owl:inverseOf :isHotelOf ;
          rdfs:domain :City ;
          rdfs:range :Hotel ;
          rdfs:comment "Lists hotels available in a city." ;
          rdfs:label "hasHotel" .


###  http://smarttourismontology.org#hasRestaurant
:hasRestaurant rdf:type owl:ObjectProperty ;
               owl:inverseOf :isRestaurantOf ;
               rdfs:domain :City ;
               rdfs:range :Restaurant ;
               rdfs:comment "Lists restaurants available in a city." ;
               rdfs:label "hasRestaurant" .


###  http://smarttourismontology.org#hasTransport
:hasTransport rdf:type owl:ObjectProperty ;
              owl:inverseOf :isTransportOf ;
              rdfs:domain :City ;
              rdfs:range :Transport ;
              rdfs:comment "Lists transport services available in a city." ;
              rdfs:label "hasTransport" .


###  http://smarttourismontology.org#isAttractionOf
:isAttractionOf rdf:type owl:ObjectProperty ;
                rdfs:domain :Attraction ;
                rdfs:range :City ;
                rdfs:comment "Inverse of hasAttraction." ,
                             "Links an attraction to the city it belongs to." ;
                rdfs:label "isAttractionOf" .


###  http://smarttourismontology.org#isEventOf
:isEventOf rdf:type owl:ObjectProperty ;
           rdfs:domain :Event ;
           rdfs:range :City ;
           rdfs:comment "Inverse of hasEvent." ,
                        "Links an event to the city it belongs to." ;
           rdfs:label "isEventOf" .


###  http://smarttourismontology.org#isHotelOf
:isHotelOf rdf:type owl:ObjectProperty ;
           rdfs:domain :Hotel ;
           rdfs:range :City ;
           rdfs:comment "Inverse of hasHotel." ,
                        "Links a hotel to the city it belongs to." ;
           rdfs:label "isHotelOf" .


###  http://smarttourismontology.org#isLocatedIn
:isLocatedIn rdf:type owl:ObjectProperty ;
             rdfs:domain :TourismEntity ;
             rdfs:range :City ;
             rdfs:comment "Inverse of hasCity." ,
                          "Links a city to an entity located in it." ;
             rdfs:label "isLocatedIn" .


###  http://smarttourismontology.org#isNearby
:isNearby rdf:type owl:ObjectProperty ;
          rdfs:domain :Hotel ;
          rdfs:range :Attraction ;
          rdfs:comment "Indicates that a hotel and an attraction are near each other." ;
          rdfs:label "isNearby" .


###  http://smarttourismontology.org#isRestaurantOf
:isRestaurantOf rdf:type owl:ObjectProperty ;
                rdfs:domain :Restaurant ;
                rdfs:range :City ;
                rdfs:comment "Inverse of hasRestaurant." ,
                             "Links a restaurant to the city it belongs to." ;
                rdfs:label "isRestaurantOf" .


###  http://smarttourismontology.org#isTransportOf
:isTransportOf rdf:type owl:ObjectProperty ;
               rdfs:domain :Transport ;
               rdfs:range :City ;
               rdfs:comment "Inverse of hasTransport." ,
                            "Links transport to the city it belongs to." ;
               rdfs:label "isTransportOf" .


#################################################################
#    Data properties
#################################################################

###  http://smarttourismontology.org#hasAverageCost
:hasAverageCost rdf:type owl:DatatypeProperty ;
                rdfs:domain :Transport ;
                rdfs:range xsd:double ;
                rdfs:comment "Average cost of using this transport mode." ;
                rdfs:label "hasAverageCost" .


###  http://smarttourismontology.org#hasAveragePrice
:hasAveragePrice rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Restaurant ;
                 rdfs:range xsd:double ;
                 rdfs:comment "Average price per meal." ;
                 rdfs:label "hasAveragePrice" .


###  http://smarttourismontology.org#hasCoordinates
:hasCoordinates rdf:type owl:DatatypeProperty ;
                rdfs:domain :City ;
                rdfs:range xsd:string ;
                rdfs:comment "Geographic coordinates in lat, long format." ;
                rdfs:label "hasCoordinates" .


###  http://smarttourismontology.org#hasDate
:hasDate rdf:type owl:DatatypeProperty ;
         rdfs:domain :Event ;
         rdfs:range xsd:date ;
         rdfs:comment "Date when the event takes place." ;
         rdfs:label "hasDate" .


###  http://smarttourismontology.org#hasOperator
:hasOperator rdf:type owl:DatatypeProperty ;
             rdfs:domain :Transport ;
             rdfs:range xsd:string ;
             rdfs:comment "Name of the company operating the transport." ;
             rdfs:label "hasOperator" .


###  http://smarttourismontology.org#hasPricePerNight
:hasPricePerNight rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Hotel ;
                  rdfs:range xsd:double ;
                  rdfs:comment "Average price per night." ;
                  rdfs:label "hasPricePerNight" .


###  http://smarttourismontology.org#hasRating
:hasRating rdf:type owl:DatatypeProperty ;
           rdfs:domain :TourismEntity ;
           rdfs:range xsd:double ;
           rdfs:comment "User-based average rating of the entity." ;
           rdfs:label "hasRating" .


###  http://smarttourismontology.org#hasStars
:hasStars rdf:type owl:DatatypeProperty ;
          rdfs:domain :Hotel ;
          rdfs:range xsd:integer ;
          rdfs:comment "Star rating of the hotel (1 to 5)." ;
          rdfs:label "hasStars" .


#################################################################
#    Classes
#################################################################

###  http://smarttourismontology.org#Attraction
:Attraction rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :hasRating ;
                              owl:minCardinality "1"^^xsd:nonNegativeInteger
                            ] .


###  http://smarttourismontology.org#Beach
:Beach rdf:type owl:Class ;
       rdfs:subClassOf :Attraction ;
       rdfs:comment "A beach is a landform alongside a body of water which consists of loose particles like sand or gravel." ;
       rdfs:label "Beach" .


###  http://smarttourismontology.org#BeachHotel
:BeachHotel rdf:type owl:Class ;
            rdfs:subClassOf :ByLocation ;
            rdfs:comment "A beach hotel is a hotel located near or on a beach, often catering to tourists seeking coastal experiences." ;
            rdfs:label "BeachHotel" .


###  http://smarttourismontology.org#BudgetHotel
:BudgetHotel rdf:type owl:Class ;
             rdfs:subClassOf :ByCategory ;
             owl:disjointWith :LuxuryHotel ;
             rdfs:comment "A budget hotel is an affordable hotel offering basic accommodations and services." ;
             rdfs:label "BudgetHotel" .


###  http://smarttourismontology.org#Bus
:Bus rdf:type owl:Class ;
     rdfs:subClassOf :Transport ;
     rdfs:comment "A bus is a road vehicle designed to carry many passengers along a set route." ;
     rdfs:label "Bus" .


###  http://smarttourismontology.org#ByCategory
:ByCategory rdf:type owl:Class ;
            rdfs:subClassOf :Hotel ;
            rdfs:comment "A grouping for hotels based on service category, such as luxury or budget." ;
            rdfs:label "ByCategory" .


###  http://smarttourismontology.org#ByLocation
:ByLocation rdf:type owl:Class ;
            rdfs:subClassOf :Hotel ;
            rdfs:comment "A grouping for hotels based on their location or surroundings." ;
            rdfs:label "ByLocation" .


###  http://smarttourismontology.org#City
:City rdf:type owl:Class .


###  http://smarttourismontology.org#CulturalEvent
:CulturalEvent rdf:type owl:Class ;
               rdfs:subClassOf :Event .


###  http://smarttourismontology.org#Event
:Event rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :hasDate ;
                         owl:cardinality "1"^^xsd:nonNegativeInteger
                       ] .


###  http://smarttourismontology.org#GastronomicEvent
:GastronomicEvent rdf:type owl:Class ;
                  rdfs:subClassOf :Event ;
                  rdfs:comment "A gastronomic event, such as food festivals, tasting events, or culinary fairs." ;
                  rdfs:label "Gastronomic Event" .


###  http://smarttourismontology.org#Hotel
:Hotel rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :hasRating ;
                         owl:minCardinality "1"^^xsd:nonNegativeInteger
                       ] .


###  http://smarttourismontology.org#ItalianRestaurant
:ItalianRestaurant rdf:type owl:Class ;
                   rdfs:subClassOf :Restaurant ;
                   owl:disjointWith :JapaneseRestaurant ,
                                    :MexicanRestaurant ;
                   rdfs:comment "An Italian restaurant serves cuisine originating from Italy." ;
                   rdfs:label "ItalianRestaurant" .


###  http://smarttourismontology.org#JapaneseRestaurant
:JapaneseRestaurant rdf:type owl:Class ;
                    rdfs:subClassOf :Restaurant ;
                    owl:disjointWith :MexicanRestaurant ;
                    rdfs:comment "A Japanese restaurant serves cuisine originating from Japan." ;
                    rdfs:label "JapaneseRestaurant" .


###  http://smarttourismontology.org#LuxuryHotel
:LuxuryHotel rdf:type owl:Class ;
             rdfs:subClassOf :ByCategory ;
             rdfs:comment "A luxury hotel is a high-end hotel that offers premium services, comfort, and amenities." ;
             rdfs:label "LuxuryHotel" .


###  http://smarttourismontology.org#Metro
:Metro rdf:type owl:Class ;
       rdfs:subClassOf :Transport .


###  http://smarttourismontology.org#MexicanRestaurant
:MexicanRestaurant rdf:type owl:Class ;
                   rdfs:subClassOf :Restaurant ;
                   rdfs:comment "A Mexican restaurant serves cuisine originating from Mexico." ;
                   rdfs:label "MexicanRestaurant" .


###  http://smarttourismontology.org#Monument
:Monument rdf:type owl:Class ;
          rdfs:subClassOf :Attraction .


###  http://smarttourismontology.org#Museum
:Museum rdf:type owl:Class ;
        rdfs:subClassOf :Attraction .


###  http://smarttourismontology.org#MusicEvent
:MusicEvent rdf:type owl:Class ;
            rdfs:subClassOf :Event .


###  http://smarttourismontology.org#Park
:Park rdf:type owl:Class ;
      rdfs:subClassOf :Attraction .


###  http://smarttourismontology.org#RecommendedHotel
:RecommendedHotel rdf:type owl:Class ;
                  rdfs:subClassOf :Hotel ;
                  rdfs:comment "A hotel recommended based on user rating." ;
                  rdfs:label "RecommendedHotel" .


###  http://smarttourismontology.org#Restaurant
:Restaurant rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :hasRating ;
                              owl:minCardinality "1"^^xsd:nonNegativeInteger
                            ] .


###  http://smarttourismontology.org#SportsEvent
:SportsEvent rdf:type owl:Class ;
             rdfs:subClassOf :Event ;
             rdfs:comment "A sports event, such as matches, tournaments, or athletic competitions." ;
             rdfs:label "Sports Event" .


###  http://smarttourismontology.org#Taxi
:Taxi rdf:type owl:Class ;
      rdfs:subClassOf :Transport .


###  http://smarttourismontology.org#TourismEntity
:TourismEntity rdf:type owl:Class ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasCity ;
                                 owl:cardinality "1"^^xsd:nonNegativeInteger
                               ] ;
               rdfs:comment "An abstract class representing any touristic entity like Hotel, Restaurant, Event, etc." ;
               rdfs:label "TourismEntity" .


###  http://smarttourismontology.org#Tram
:Tram rdf:type owl:Class ;
      rdfs:subClassOf :Transport .


###  http://smarttourismontology.org#Transport
:Transport rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :hasAverageCost ;
                             owl:minCardinality "1"^^xsd:nonNegativeInteger
                           ] .


###  http://smarttourismontology.org#UrbanHotel
:UrbanHotel rdf:type owl:Class ;
            rdfs:subClassOf :ByLocation ;
            rdfs:comment "An urban hotel is located in a city center and typically serves business or city-traveling guests." ;
            rdfs:label "UrbanHotel" .


