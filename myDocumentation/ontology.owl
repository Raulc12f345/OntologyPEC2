<?xml version="1.0"?>
<rdf:RDF xmlns="http://smarttourismontology.org#"
     xml:base="http://smarttourismontology.org"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:smarttourismontology="http://smarttourismontology.org#">
    <owl:Ontology rdf:about="http://smarttourismontology.org">
        <owl:versionIRI rdf:resource="http://smarttourismontology.org/1.0.0"/>
        <terms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-04-20</terms:created>
        <terms:creator>Raúl Castillo Hernández</terms:creator>
        <terms:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
        <rdfs:comment>An ontology for representing smart tourism data and recommendations by place.</rdfs:comment>
        <rdfs:label>Smart Tourism Ontology</rdfs:label>
        <owl:versionInfo>v1.0.0</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/terms/created -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/created"/>
    


    <!-- http://purl.org/dc/terms/creator -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/creator"/>
    


    <!-- http://purl.org/dc/terms/license -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
    


    <!-- http://smarttourismontology.org#hasName -->


    <owl:AnnotationProperty rdf:about="http://smarttourismontology.org#hasName"/>
    


    <!-- http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled -->


    <owl:AnnotationProperty rdf:about="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled"/>
    


    <!-- http://www.w3.org/2002/07/owl#cardinality -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#cardinality"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/2001/XMLSchema#date -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://smarttourismontology.org#hasAttraction -->


    <owl:ObjectProperty rdf:about="http://smarttourismontology.org#hasAttraction">
        <owl:inverseOf rdf:resource="http://smarttourismontology.org#isAttractionOf"/>
        <rdfs:domain rdf:resource="http://smarttourismontology.org#City"/>
        <rdfs:range rdf:resource="http://smarttourismontology.org#Attraction"/>
        <rdfs:comment>Lists attractions available in a city.</rdfs:comment>
        <rdfs:label>hasAttraction</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://smarttourismontology.org#hasCity -->


    <owl:ObjectProperty rdf:about="http://smarttourismontology.org#hasCity">
        <rdfs:domain rdf:resource="http://smarttourismontology.org#TourismEntity"/>
        <rdfs:range rdf:resource="http://smarttourismontology.org#City"/>
        <rdfs:comment>Links an entity to the city where it is located.</rdfs:comment>
        <rdfs:label>hasCity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://smarttourismontology.org#hasEvent -->


    <owl:ObjectProperty rdf:about="http://smarttourismontology.org#hasEvent">
        <owl:inverseOf rdf:resource="http://smarttourismontology.org#isEventOf"/>
        <rdfs:domain rdf:resource="http://smarttourismontology.org#City"/>
        <rdfs:range rdf:resource="http://smarttourismontology.org#Event"/>
        <rdfs:comment>States which events take place in a city.</rdfs:comment>
        <rdfs:label>hasEvent</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://smarttourismontology.org#hasHotel -->


    <owl:ObjectProperty rdf:about="http://smarttourismontology.org#hasHotel">
        <owl:inverseOf rdf:resource="http://smarttourismontology.org#isHotelOf"/>
        <rdfs:domain rdf:resource="http://smarttourismontology.org#City"/>
        <rdfs:range rdf:resource="http://smarttourismontology.org#Hotel"/>
        <rdfs:comment>Lists hotels available in a city.</rdfs:comment>
        <rdfs:label>hasHotel</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://smarttourismontology.org#hasRestaurant -->


    <owl:ObjectProperty rdf:about="http://smarttourismontology.org#hasRestaurant">
        <owl:inverseOf rdf:resource="http://smarttourismontology.org#isRestaurantOf"/>
        <rdfs:domain rdf:resource="http://smarttourismontology.org#City"/>
        <rdfs:range rdf:resource="http://smarttourismontology.org#Restaurant"/>
        <rdfs:comment>Lists restaurants available in a city.</rdfs:comment>
        <rdfs:label>hasRestaurant</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://smarttourismontology.org#hasTransport -->


    <owl:ObjectProperty rdf:about="http://smarttourismontology.org#hasTransport">
        <owl:inverseOf rdf:resource="http://smarttourismontology.org#isTransportOf"/>
        <rdfs:domain rdf:resource="http://smarttourismontology.org#City"/>
        <rdfs:range rdf:resource="http://smarttourismontology.org#Transport"/>
        <rdfs:comment>Lists transport services available in a city.</rdfs:comment>
        <rdfs:label>hasTransport</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://smarttourismontology.org#isAttractionOf -->


    <owl:ObjectProperty rdf:about="http://smarttourismontology.org#isAttractionOf">
        <rdfs:domain rdf:resource="http://smarttourismontology.org#Attraction"/>
        <rdfs:range rdf:resource="http://smarttourismontology.org#City"/>
        <rdfs:comment>Inverse of hasAttraction.</rdfs:comment>
        <rdfs:comment>Links an attraction to the city it belongs to.</rdfs:comment>
        <rdfs:label>isAttractionOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://smarttourismontology.org#isEventOf -->


    <owl:ObjectProperty rdf:about="http://smarttourismontology.org#isEventOf">
        <rdfs:domain rdf:resource="http://smarttourismontology.org#Event"/>
        <rdfs:range rdf:resource="http://smarttourismontology.org#City"/>
        <rdfs:comment>Inverse of hasEvent.</rdfs:comment>
        <rdfs:comment>Links an event to the city it belongs to.</rdfs:comment>
        <rdfs:label>isEventOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://smarttourismontology.org#isHotelOf -->


    <owl:ObjectProperty rdf:about="http://smarttourismontology.org#isHotelOf">
        <rdfs:domain rdf:resource="http://smarttourismontology.org#Hotel"/>
        <rdfs:range rdf:resource="http://smarttourismontology.org#City"/>
        <rdfs:comment>Inverse of hasHotel.</rdfs:comment>
        <rdfs:comment>Links a hotel to the city it belongs to.</rdfs:comment>
        <rdfs:label>isHotelOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://smarttourismontology.org#isLocatedIn -->


    <owl:ObjectProperty rdf:about="http://smarttourismontology.org#isLocatedIn">
        <rdfs:domain rdf:resource="http://smarttourismontology.org#TourismEntity"/>
        <rdfs:range rdf:resource="http://smarttourismontology.org#City"/>
        <rdfs:comment>Inverse of hasCity.</rdfs:comment>
        <rdfs:comment>Links a city to an entity located in it.</rdfs:comment>
        <rdfs:label>isLocatedIn</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://smarttourismontology.org#isNearby -->


    <owl:ObjectProperty rdf:about="http://smarttourismontology.org#isNearby">
        <rdfs:domain rdf:resource="http://smarttourismontology.org#Hotel"/>
        <rdfs:range rdf:resource="http://smarttourismontology.org#Attraction"/>
        <rdfs:comment>Indicates that a hotel and an attraction are near each other.</rdfs:comment>
        <rdfs:label>isNearby</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://smarttourismontology.org#isRestaurantOf -->


    <owl:ObjectProperty rdf:about="http://smarttourismontology.org#isRestaurantOf">
        <rdfs:domain rdf:resource="http://smarttourismontology.org#Restaurant"/>
        <rdfs:range rdf:resource="http://smarttourismontology.org#City"/>
        <rdfs:comment>Inverse of hasRestaurant.</rdfs:comment>
        <rdfs:comment>Links a restaurant to the city it belongs to.</rdfs:comment>
        <rdfs:label>isRestaurantOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://smarttourismontology.org#isTransportOf -->


    <owl:ObjectProperty rdf:about="http://smarttourismontology.org#isTransportOf">
        <rdfs:domain rdf:resource="http://smarttourismontology.org#Transport"/>
        <rdfs:range rdf:resource="http://smarttourismontology.org#City"/>
        <rdfs:comment>Inverse of hasTransport.</rdfs:comment>
        <rdfs:comment>Links transport to the city it belongs to.</rdfs:comment>
        <rdfs:label>isTransportOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://smarttourismontology.org#hasAverageCost -->


    <owl:DatatypeProperty rdf:about="http://smarttourismontology.org#hasAverageCost">
        <rdfs:domain rdf:resource="http://smarttourismontology.org#Transport"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>Average cost of using this transport mode.</rdfs:comment>
        <rdfs:label>hasAverageCost</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://smarttourismontology.org#hasAveragePrice -->


    <owl:DatatypeProperty rdf:about="http://smarttourismontology.org#hasAveragePrice">
        <rdfs:domain rdf:resource="http://smarttourismontology.org#Restaurant"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>Average price per meal.</rdfs:comment>
        <rdfs:label>hasAveragePrice</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://smarttourismontology.org#hasCoordinates -->


    <owl:DatatypeProperty rdf:about="http://smarttourismontology.org#hasCoordinates">
        <rdfs:domain rdf:resource="http://smarttourismontology.org#City"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Geographic coordinates in lat, long format.</rdfs:comment>
        <rdfs:label>hasCoordinates</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://smarttourismontology.org#hasDate -->


    <owl:DatatypeProperty rdf:about="http://smarttourismontology.org#hasDate">
        <rdfs:domain rdf:resource="http://smarttourismontology.org#Event"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdfs:comment>Date when the event takes place.</rdfs:comment>
        <rdfs:label>hasDate</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://smarttourismontology.org#hasOperator -->


    <owl:DatatypeProperty rdf:about="http://smarttourismontology.org#hasOperator">
        <rdfs:domain rdf:resource="http://smarttourismontology.org#Transport"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Name of the company operating the transport.</rdfs:comment>
        <rdfs:label>hasOperator</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://smarttourismontology.org#hasPricePerNight -->


    <owl:DatatypeProperty rdf:about="http://smarttourismontology.org#hasPricePerNight">
        <rdfs:domain rdf:resource="http://smarttourismontology.org#Hotel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>Average price per night.</rdfs:comment>
        <rdfs:label>hasPricePerNight</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://smarttourismontology.org#hasRating -->


    <owl:DatatypeProperty rdf:about="http://smarttourismontology.org#hasRating">
        <rdfs:domain rdf:resource="http://smarttourismontology.org#TourismEntity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>User-based average rating of the entity.</rdfs:comment>
        <rdfs:label>hasRating</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://smarttourismontology.org#hasStars -->


    <owl:DatatypeProperty rdf:about="http://smarttourismontology.org#hasStars">
        <rdfs:domain rdf:resource="http://smarttourismontology.org#Hotel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Star rating of the hotel (1 to 5).</rdfs:comment>
        <rdfs:label>hasStars</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://smarttourismontology.org#Attraction -->


    <owl:Class rdf:about="http://smarttourismontology.org#Attraction">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smarttourismontology.org#hasRating"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://smarttourismontology.org#Beach -->


    <owl:Class rdf:about="http://smarttourismontology.org#Beach">
        <rdfs:subClassOf rdf:resource="http://smarttourismontology.org#Attraction"/>
        <rdfs:comment>A beach is a landform alongside a body of water which consists of loose particles like sand or gravel.</rdfs:comment>
        <rdfs:label>Beach</rdfs:label>
    </owl:Class>
    


    <!-- http://smarttourismontology.org#BeachHotel -->


    <owl:Class rdf:about="http://smarttourismontology.org#BeachHotel">
        <rdfs:subClassOf rdf:resource="http://smarttourismontology.org#ByLocation"/>
        <rdfs:comment>A beach hotel is a hotel located near or on a beach, often catering to tourists seeking coastal experiences.</rdfs:comment>
        <rdfs:label>BeachHotel</rdfs:label>
    </owl:Class>
    


    <!-- http://smarttourismontology.org#BudgetHotel -->


    <owl:Class rdf:about="http://smarttourismontology.org#BudgetHotel">
        <rdfs:subClassOf rdf:resource="http://smarttourismontology.org#ByCategory"/>
        <owl:disjointWith rdf:resource="http://smarttourismontology.org#LuxuryHotel"/>
        <rdfs:comment>A budget hotel is an affordable hotel offering basic accommodations and services.</rdfs:comment>
        <rdfs:label>BudgetHotel</rdfs:label>
    </owl:Class>
    


    <!-- http://smarttourismontology.org#Bus -->


    <owl:Class rdf:about="http://smarttourismontology.org#Bus">
        <rdfs:subClassOf rdf:resource="http://smarttourismontology.org#Transport"/>
        <rdfs:comment>A bus is a road vehicle designed to carry many passengers along a set route.</rdfs:comment>
        <rdfs:label>Bus</rdfs:label>
    </owl:Class>
    


    <!-- http://smarttourismontology.org#ByCategory -->


    <owl:Class rdf:about="http://smarttourismontology.org#ByCategory">
        <rdfs:subClassOf rdf:resource="http://smarttourismontology.org#Hotel"/>
        <rdfs:comment>A grouping for hotels based on service category, such as luxury or budget.</rdfs:comment>
        <rdfs:label>ByCategory</rdfs:label>
    </owl:Class>
    


    <!-- http://smarttourismontology.org#ByLocation -->


    <owl:Class rdf:about="http://smarttourismontology.org#ByLocation">
        <rdfs:subClassOf rdf:resource="http://smarttourismontology.org#Hotel"/>
        <rdfs:comment>A grouping for hotels based on their location or surroundings.</rdfs:comment>
        <rdfs:label>ByLocation</rdfs:label>
    </owl:Class>
    


    <!-- http://smarttourismontology.org#City -->


    <owl:Class rdf:about="http://smarttourismontology.org#City"/>
    


    <!-- http://smarttourismontology.org#CulturalEvent -->


    <owl:Class rdf:about="http://smarttourismontology.org#CulturalEvent">
        <rdfs:subClassOf rdf:resource="http://smarttourismontology.org#Event"/>
    </owl:Class>
    


    <!-- http://smarttourismontology.org#Event -->


    <owl:Class rdf:about="http://smarttourismontology.org#Event">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smarttourismontology.org#hasDate"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://smarttourismontology.org#GastronomicEvent -->


    <owl:Class rdf:about="http://smarttourismontology.org#GastronomicEvent">
        <rdfs:subClassOf rdf:resource="http://smarttourismontology.org#Event"/>
        <rdfs:comment>A gastronomic event, such as food festivals, tasting events, or culinary fairs.</rdfs:comment>
        <rdfs:label>Gastronomic Event</rdfs:label>
    </owl:Class>
    


    <!-- http://smarttourismontology.org#Hotel -->


    <owl:Class rdf:about="http://smarttourismontology.org#Hotel">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smarttourismontology.org#hasRating"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://smarttourismontology.org#ItalianRestaurant -->


    <owl:Class rdf:about="http://smarttourismontology.org#ItalianRestaurant">
        <rdfs:subClassOf rdf:resource="http://smarttourismontology.org#Restaurant"/>
        <owl:disjointWith rdf:resource="http://smarttourismontology.org#JapaneseRestaurant"/>
        <owl:disjointWith rdf:resource="http://smarttourismontology.org#MexicanRestaurant"/>
        <rdfs:comment>An Italian restaurant serves cuisine originating from Italy.</rdfs:comment>
        <rdfs:label>ItalianRestaurant</rdfs:label>
    </owl:Class>
    


    <!-- http://smarttourismontology.org#JapaneseRestaurant -->


    <owl:Class rdf:about="http://smarttourismontology.org#JapaneseRestaurant">
        <rdfs:subClassOf rdf:resource="http://smarttourismontology.org#Restaurant"/>
        <owl:disjointWith rdf:resource="http://smarttourismontology.org#MexicanRestaurant"/>
        <rdfs:comment>A Japanese restaurant serves cuisine originating from Japan.</rdfs:comment>
        <rdfs:label>JapaneseRestaurant</rdfs:label>
    </owl:Class>
    


    <!-- http://smarttourismontology.org#LuxuryHotel -->


    <owl:Class rdf:about="http://smarttourismontology.org#LuxuryHotel">
        <rdfs:subClassOf rdf:resource="http://smarttourismontology.org#ByCategory"/>
        <rdfs:comment>A luxury hotel is a high-end hotel that offers premium services, comfort, and amenities.</rdfs:comment>
        <rdfs:label>LuxuryHotel</rdfs:label>
    </owl:Class>
    


    <!-- http://smarttourismontology.org#Metro -->


    <owl:Class rdf:about="http://smarttourismontology.org#Metro">
        <rdfs:subClassOf rdf:resource="http://smarttourismontology.org#Transport"/>
    </owl:Class>
    


    <!-- http://smarttourismontology.org#MexicanRestaurant -->


    <owl:Class rdf:about="http://smarttourismontology.org#MexicanRestaurant">
        <rdfs:subClassOf rdf:resource="http://smarttourismontology.org#Restaurant"/>
        <rdfs:comment>A Mexican restaurant serves cuisine originating from Mexico.</rdfs:comment>
        <rdfs:label>MexicanRestaurant</rdfs:label>
    </owl:Class>
    


    <!-- http://smarttourismontology.org#Monument -->


    <owl:Class rdf:about="http://smarttourismontology.org#Monument">
        <rdfs:subClassOf rdf:resource="http://smarttourismontology.org#Attraction"/>
    </owl:Class>
    


    <!-- http://smarttourismontology.org#Museum -->


    <owl:Class rdf:about="http://smarttourismontology.org#Museum">
        <rdfs:subClassOf rdf:resource="http://smarttourismontology.org#Attraction"/>
    </owl:Class>
    


    <!-- http://smarttourismontology.org#MusicEvent -->


    <owl:Class rdf:about="http://smarttourismontology.org#MusicEvent">
        <rdfs:subClassOf rdf:resource="http://smarttourismontology.org#Event"/>
    </owl:Class>
    


    <!-- http://smarttourismontology.org#Park -->


    <owl:Class rdf:about="http://smarttourismontology.org#Park">
        <rdfs:subClassOf rdf:resource="http://smarttourismontology.org#Attraction"/>
    </owl:Class>
    


    <!-- http://smarttourismontology.org#RecommendedHotel -->


    <owl:Class rdf:about="http://smarttourismontology.org#RecommendedHotel">
        <rdfs:subClassOf rdf:resource="http://smarttourismontology.org#Hotel"/>
        <rdfs:comment>A hotel recommended based on user rating.</rdfs:comment>
        <rdfs:label>RecommendedHotel</rdfs:label>
    </owl:Class>
    


    <!-- http://smarttourismontology.org#Restaurant -->


    <owl:Class rdf:about="http://smarttourismontology.org#Restaurant">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smarttourismontology.org#hasRating"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://smarttourismontology.org#SportsEvent -->


    <owl:Class rdf:about="http://smarttourismontology.org#SportsEvent">
        <rdfs:subClassOf rdf:resource="http://smarttourismontology.org#Event"/>
        <rdfs:comment>A sports event, such as matches, tournaments, or athletic competitions.</rdfs:comment>
        <rdfs:label>Sports Event</rdfs:label>
    </owl:Class>
    


    <!-- http://smarttourismontology.org#Taxi -->


    <owl:Class rdf:about="http://smarttourismontology.org#Taxi">
        <rdfs:subClassOf rdf:resource="http://smarttourismontology.org#Transport"/>
    </owl:Class>
    


    <!-- http://smarttourismontology.org#TourismEntity -->


    <owl:Class rdf:about="http://smarttourismontology.org#TourismEntity">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smarttourismontology.org#hasCity"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>An abstract class representing any touristic entity like Hotel, Restaurant, Event, etc.</rdfs:comment>
        <rdfs:label>TourismEntity</rdfs:label>
    </owl:Class>
    


    <!-- http://smarttourismontology.org#Tram -->


    <owl:Class rdf:about="http://smarttourismontology.org#Tram">
        <rdfs:subClassOf rdf:resource="http://smarttourismontology.org#Transport"/>
    </owl:Class>
    


    <!-- http://smarttourismontology.org#Transport -->


    <owl:Class rdf:about="http://smarttourismontology.org#Transport">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smarttourismontology.org#hasAverageCost"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://smarttourismontology.org#UrbanHotel -->


    <owl:Class rdf:about="http://smarttourismontology.org#UrbanHotel">
        <rdfs:subClassOf rdf:resource="http://smarttourismontology.org#ByLocation"/>
        <rdfs:comment>An urban hotel is located in a city center and typically serves business or city-traveling guests.</rdfs:comment>
        <rdfs:label>UrbanHotel</rdfs:label>
    </owl:Class>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


